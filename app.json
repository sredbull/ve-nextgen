{
  "name": "ve-nextgen",
  "repository": "https://github.com/sredbull/ve-nextgen",
  "env": {
    "NODE_ENV": {
      "value": "production"
    },
    "JWT_SECRET": {
      "generator": "secret"
    },
    "JWT_EXPIRES_IN": {
      "value": "10d"
    },
    "OAUTH_GOOGLE_ID": {
      "value": "dummy"
    },
    "OAUTH_GOOGLE_SECRET": {
      "value": "dummy"
    },
    "OAUTH_GOOGLE_REDIRECT_URL": {
      "value": "dummy"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "13"
      }
    }
  ],
  "stack": "container"
}
